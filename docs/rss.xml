int main()
{
	int a = 0;
	scanf_s('%d', &amp;a);
	if (a &lt;= 10)
		printf('小于等于10\n');
	else if (a &gt; 10 &amp;&amp; a &lt;= 20)
		printf('大于10小于等于20\n');
	else if (a &gt; 20 &amp;&amp; a &lt;= 30)
		printf('大于20小于等于30\n');
	else if (a &gt; 30 &amp;&amp; a &lt;= 40)
		printf('大于30小于等于40\n');
	else
		printf('大于40\n');

	return 0;
}


